{
  "name": "node-ddd-template",
  "version": "1.0.0",
  "description": "Node template for building apps using DDD",
  "main": "index.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "dev": "nodemon --exec node -r ts-node/register -r tsconfig-paths/register --env-file=.env src/index.ts",
    "build": "rimraf build && tsc",
    "start": "node --env-file=.env build/index.js",
    "serve": "npm run build && npm run start",
    "sq": "sequelize-cli",
    "lint": "eslint --ignore-path .gitignore --ext .ts",
    "format": "prettier --ignore-path .gitignore --write \"**/*.+(js|ts|json)\""
  },
  "repository": {
    "type": "git",
    "url": "git@github.com-toritsejufo:toritsejuFO/node-ddd-template.git"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "awilix": "^6.0.0",
    "bcrypt": "^5.0.1",
    "express": "^4.17.2",
    "http-status-codes": "^2.2.0",
    "jsonwebtoken": "^8.5.1",
    "log4js": "^6.3.0",
    "mysql2": "^2.3.3",
    "nodemailer": "^6.7.2",
    "pg": "^8.12.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.37.3",
    "types-ddd": "^3.9.2",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0-alpha.3",
    "@types/bcrypt": "^5.0.2",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "^20.14.2",
    "@types/nodemailer": "^6.4.15",
    "@types/pg": "^8.11.6",
    "@typescript-eslint/eslint-plugin": "^7.13.0",
    "@typescript-eslint/parser": "^7.13.0",
    "eslint": "^8.57.0",
    "eslint-config-prettier": "^9.1.0",
    "eslint-plugin-prettier": "^5.1.3",
    "nodemon": "^2.0.15",
    "rimraf": "^5.0.7",
    "sequelize-cli": "^6.3.0",
    "ts-node": "^10.9.2",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.4.5"
  }
}
